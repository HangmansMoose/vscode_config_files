// Directly run the exe
{
    "version":"0.2.0",
    "configurations": [
        {
            "name":"C Launch",
            "type": "cppvsdbg",
            "request": "launch",
            "program":"${workspaceRoot}/bin/raycast.exe",
            "args":[],
            "cwd":"${workspaceFolder}",
            "console":"integratedTerminal"
        }
    ]
}

//Rust 
{
      "name": "rust launch",
      "type": "lldb",
      "request": "launch",
      "console": "integratedTerminal",
      "cargo": {
        "args": ["build", "--manifest-path", "${fileDirname}/../Cargo.toml"]
      }
}

